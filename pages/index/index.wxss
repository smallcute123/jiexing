.cost-content{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  /* height: 100vh; */
  width: 100%;
  margin-top: -35rpx;
  background-color: #f4f6f8;
}
.cost-addr{
  height: 360rpx;
  width: 100%;
  /* background: red; */
 
}
.star-addr{
  position: absolute;
  left: 80rpx;
  top: 120rpx;
  width: 100%;
}
.star-addr::after{
  content: " ";
    display: inline-block;
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #ccc;
    border-style: solid;
    transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
    position: absolute;
    top: 50%;
    left: 580rpx;
    margin-top: -4px;
}

.end-addr{
  position: absolute;
  left: 80rpx;
  top: 230rpx;
  width: 100%;
}
.end-addr::after{
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #ccc;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  left: 580rpx;
  margin-top: -4px;
}
